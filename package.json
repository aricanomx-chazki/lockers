{
  "name": "demo-locker-app",
  "author": "Chazki",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "jest --watchAll --coverage --detectOpenHandles --forceExit",
    "test-push": "jest --coverage --detectOpenHandles --forceExit",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "pre-commit": "husky install",
    "deploy-dist": "gh-pages -d dist",
    "deploy-storybook": "gh-pages -d storybook-static",
    "start": "serve dist -s -n -L -p $PORT"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test-push"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@otplib/preset-browser": "^12.0.1",
    "@railway/cli": "^3.5.1",
    "@yudiel/react-qr-scanner": "^1.1.10",
    "antd": "^5.10.2",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "i18next": "^23.6.0",
    "lint-staged": "^14.0.1",
    "npm": "^10.2.1",
    "otplib": "^12.0.1",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.1",
    "react-router-dom": "^6.17.0",
    "serve": "^14.2.1",
    "styled-components": "^6.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^29.5.6",
    "@types/lint-staged": "^13.2.1",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "jsdom": "^22.1.0",
    "storybook": "^7.5.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "resolutions": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.7.0"
  }
}
